# HC Configuration
#
# Date: 14/3/2005
# Author: xxx
#


# HC public home address (also a non-routable address)
host_addr=3110529001
#host_addr=0829962
#host_addr=527523287

# HC external SIP port (used on external interfaces)
host_port=5060 #5070  EN sip port used by the client

# HC internal (local) address (the same address used in x-lite configuration for outbound proxy)
local_addr=127.0.0.1

# HC internal (local) SIP port (the same port used in x-lite configuration for outbound proxy)
local_port=5060 # 5050

# Outbound proxy (use the default GW address/port)
use_outbound=yes
outbound_addr=160.80.103.5 #.26
gw_port=5060 # prima era 50070

# UDP keep-alive packets (for NAT refreshing)
use_udp_keepalive=yes
# SIP be-in-touch messages (for Contact binding)
use_sip_beintouch=yes

# HC timeouts
relay_timeout=10000
udp_keepalive_time=2000
sip_beintouch_time=5000


#update to new signaling (november 2006)
#use_tid_parameter=yes
use_handover_join_header=yes


# HC media ports
media_ports=50080:50099

# SIP timeouts
retransmission_timeout=2000
max_retransmission_timeout=4000
transaction_timeout=10000
clearing_timeout=5000

# Debugging and logs
debug_level=5
log_path=log
log_file=hc.log
max_logsize=800
log_rotations=3
log_rotation_time=7 DAYS
